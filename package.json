{
  "name": "@cinic/forest-url-router",
  "version": "2.0.5",
  "description": "Small url router for Forest JS",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/cinic/forest-url-router.git",
  "author": "Alexander Andreev <cinic.rus@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn clean && yarn compile && yarn copyts",
    "compile": "rollup --config",
    "types": "tsc --build tsconfig.build.json",
    "copyts": "cp ./src/index.d.ts ./dist/index.d.ts && cp ./src/index.d.ts ./dist/index.esm.d.ts && cp ./src/index.d.ts ./dist/index.cjs.d.ts",
    "clean": "rm -rf ./dist",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "files": [
    "dist/index.cjs.js",
    "dist/index.esm.js",
    "dist/index.d.ts",
    "dist/index.esm.d.ts",
    "dist/index.cjs.d.ts",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "path-to-regexp": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-jest": "^27.0.1",
    "effector": "^21.8.11",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.2.0",
    "forest": "^0.19.6",
    "husky": "^4.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rollup": "^2.34.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "effector": "^21.7.5",
    "forest": "^0.19.2"
  },
  "publishConfig": {
    "registry":"https://npm.pkg.github.com"
  }
}
