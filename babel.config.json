{
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "allExtensions": true
      }
    ]
  ],
  "plugins": [
    [
      "effector/babel-plugin",
      {
        "addLoc": true
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-typescript",
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}